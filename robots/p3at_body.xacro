<?xml version="1.0"?>


<!-- Espacios de nombres xlmns para evitar conflicots al combinar diferentes archivos xml -->

<robot xmlns:sensor="http://playerstage.sourceforge.net/gazebo/xmlschema/#sensor"
       xmlns:controller="http://playerstage.sourceforge.net/gazebo/xmlschema/#controller"
       xmlns:interface="http://playerstage.sourceforge.net/gazebo/xmlschema/#interface"
       xmlns:xacro="http://www.ros.org/wiki/xacro">

	<link name="base_link"/>
	
<!-- //////////////////////////////////////////////////////////////////////////////////////////////// -->
<!-- Chassis -->
<!-- //////////////////////////////////////////////////////////////////////////////////////////////// -->
	<link name="chassis">
		<inertial>
			<mass value="20.0"/>
			<origin xyz="0 0 -0.090"/>
			<inertia ixx="0.3338" ixy="0.0" ixz="0.0"
				 iyy="0.4783" iyz="0.0"
				 izz="0.3338"/>
		</inertial>
		<visual>
			<origin xyz="0 0 0.0" rpy="0 0 0"/>
			<geometry name="pioneer_geom">
				<mesh filename="package://mapping_pioneer3at/meshes/chassis.dae"/>
			</geometry>
				<material name="ChassisRed">
					<color rgba="0.851 0.0 0.0 1.0"/>
			</material>
		</visual>
		<collision>
			<origin xyz="0 0 0.0" rpy="0 0 0"/>
				<geometry>
					<mesh filename="package://mapping_pioneer3at/meshes/chassis.dae"/>
				</geometry>
		</collision>
	</link>
	<gazebo reference="chassis">
		<material value="Gazebo/Red"/>
	</gazebo>
	<joint name="base_chassis_joint" type="fixed">
		<origin xyz="0.00 0 0.177" rpy="0 0 0"/>
		<parent link="base_link"/>
		<child link="chassis"/>
	</joint>

<!-- //////////////////////////////////////////////////////////////////////////////////////////////// -->
<!-- Top --> <!-- //////////////////////////////////////////////////////////////////////////////////////////////// -->
	<link name="top_plate">
		<inertial>
			<mass value="0.1"/>	
			<origin xyz="-0.025 0 -0.223"/>
			<inertia ixx="1.0" ixy="0.0" ixz="0.0"
				 iyy="1.0" iyz="0.0"
 				izz="1.0"/>
		</inertial>
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry name="pioneer_geom">
				<mesh filename="package://mapping_pioneer3at/meshes/top.dae"/>
			</geometry>
			<material name="TopBlack">
					<color rgba="0.038 0.038 0.038 1.0"/>
			</material>
		</visual>
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<box size="0 0 0"/>
			</geometry>
		</collision>
	</link>
	<gazebo reference="top_plate">
		<material value="Gazebo/Black"/>
	</gazebo>

	<joint name="chassis_top_joint" type="fixed">
		<origin xyz="0.043 0 0.254" rpy="0 0 0"/>
		<parent link="base_link"/>
		<child link="top_plate"/>
	</joint>
	
	
<!-- //////////////////////////////////////////////////////////////////////////////////////////////// -->
<!-- Front + Back Sonar -->
<!-- //////////////////////////////////////////////////////////////////////////////////////////////// -->
	<link name="front_sonar">
		<inertial>
			<mass value="0.1"/>	
			<origin xyz="0 0 0"/>
			<inertia ixx="1.0" ixy="0.0" ixz="0.0"
				 iyy="1.0" iyz="0.0" izz="1.0"/>
		</inertial>
		<visual name="front_sonar_visual">
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry name="pioneer_geom">
				<mesh filename="package://mapping_pioneer3at/meshes/front_sonar.dae"/>
			</geometry>
			<material name="SonarYellow">
				<color rgba="0.715 0.583 0.210 1.0"/>
			</material>
		</visual>
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<box size="0 0 0"/>
			</geometry>
		</collision>
	</link>
	<gazebo reference="front_sonar">
		<material value="Gazebo/Yellow"/>
	</gazebo>

	<joint name="base_front_joint" type="fixed">
		<origin xyz="0.193 0 0.226" rpy="0 0 0"/>
		<parent link="base_link"/>
		<child link="front_sonar"/>
	</joint>

<!-- //////////////////////////////////////////////////////////////////////////////////////////////// -->
<!-- //////////////////////////////////////////////////////////////////////////////////////////////// -->
	<link name="back_sonar">
		<inertial>
			<mass value="0.1"/>	
			<origin xyz="0 0 0"/>
			<inertia ixx="1.0" ixy="0.0" ixz="0.0"
				 iyy="1.0" iyz="0.0" izz="1.0"/>
		</inertial>
		<visual name="back_sonar_visual">
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry name="pioneer_geom">
				<mesh filename="package://mapping_pioneer3at/meshes/back_sonar.dae"/>
			</geometry>
			<material name="SonarYellow">
				<color rgba="0.715 0.583 0.210 1.0"/>
			</material>
		</visual>
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<box size="0 0 0"/>
			</geometry>
		</collision>
	</link>
	<gazebo reference="back_sonar">
		<material value="Gazebo/Yellow"/>
	</gazebo>

	<joint name="base_back_joint" type="fixed">
		<origin xyz="-0.193 0 0.226" rpy="0 0 0"/>
		<parent link="base_link"/>
		<child link="back_sonar"/>
	</joint>




<!-- Configuracion Hokuyo SICK LMS200 -->

 <joint name="hokuyo_joint" type="fixed">
    <axis xyz="0 1 0" />
    <origin xyz="0.150 0 0.037" rpy="0 0 0"/>
    <parent link="top_plate"/>
    <child link="hokuyo_link"/>
  </joint>
  <link name="hokuyo_link">
    <collision>
      <origin xyz="0.030 0 0.0" rpy="0 0 0"/>
      <geometry>
    <box size="0.01 0.01 0.01"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0.030 0 0.0" rpy="0 0 0"/> 
      <geometry>
        <mesh filename="package://mapping_pioneer3at/meshes/hokuyo1.dae"/>
      </geometry>
    </visual>
    <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
  </link>
<gazebo reference="hokuyo_link">
    <sensor type="ray" name="head_hokuyo_sensor">
      <pose>0 0 0 0 0 0</pose>
      <visualize>true</visualize>
      <!--<update_rate>40</update_rate> ORIGINAL ESTO PARA DEJAR MENOS ESPACIOS AL SENSAR-->
      <update_rate>100</update_rate>
      <ray>
        <scan>
          <horizontal>
            <!--<samples>180</samples> ORIGINAL ESTO PARA DEJAR MENOS ESPACIOS AL SENSAR-->
            <samples>540</samples>
            <resolution>1</resolution>
            <min_angle>-1.570796</min_angle>
            <max_angle>1.570796</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.10</min>
          <max>30.0</max>
          <resolution>0.01</resolution>  
        </range>
        <noise>
          <type>gaussian</type>

          <mean>0.0</mean>
          <stddev>0.015</stddev>   <!-- Statistical error typ. 5 mm (mm-mode) -->
        </noise>
      </ray>
      <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_laser.so">
        <topicName>/scan</topicName>
        <frameName>hokuyo_link</frameName>
      </plugin>
    </sensor>
</gazebo>






